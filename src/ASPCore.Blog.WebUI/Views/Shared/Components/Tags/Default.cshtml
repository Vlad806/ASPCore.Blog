@model IEnumerable<ASPCore.Blog.WebUI.Models.TagsModel>
@{
    Layout = null;
}

<div class="single-widget tags-widget">
    <h4 class="title">Post Tags</h4>
    <ul>
        @foreach (var tag in Model)
        {
            <li><a href="javascript:void(0)" id="@tag.TagId" onclick="filteredByTag(this);return false;">@tag.Name</a></li>
        }
    </ul>
</div>

